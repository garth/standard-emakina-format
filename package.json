{
  "name": "standard-emakina-format",
  "version": "3.0.1",
  "description": "attempts to reformat javascript to comply with feross/standard style but modified for emakina",
  "main": "index.js",
  "bin": "./bin.js",
  "scripts": {
    "test": "standard && tape test/*.js | tap-spec",
    "test-file": "<test.js ./bin.js | standard-emakina",
    "failing": "standard && tape test/failing/*.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garth/standard-emakina-format.git"
  },
  "author": "max ogden",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/garth/standard-emakina-format/issues"
  },
  "homepage": "https://github.com/garth/standard-emakina-format",
  "dependencies": {
    "minimist": "^1.1.0",
    "semi": "^4.0.4",
    "standard-format": "^2.1.1",
    "stdin": "0.0.1"
  },
  "devDependencies": {
    "concat-stream": "^1.4.7",
    "debug": "^2.1.1",
    "once": "^1.3.1",
    "pre-commit": "^1.1.2",
    "standard-emakina": "^9.2.0",
    "skip-stream": "0.0.3",
    "snazzy": "^2.0.1",
    "split": "^1.0.0",
    "standard": "^8.0.0",
    "stream-reduce": "^1.0.3",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  },
  "standard": {
    "ignore": [
      "**test/test-files/**",
      "**test/failing/**"
    ]
  }
}
